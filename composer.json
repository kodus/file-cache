{
    "name": "kodus/file-cache",
    "description": "Minimal PSR-16 cache-implementation",
    "authors": [
        {
            "name": "Rasmus Schultz",
            "email": "rasc@jfmedier.dk",
            "role": "Developer"
        },
        {
            "name": "Thomas Pedersen",
            "email": "thno@jfmedier.dk",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "provide": {
        "psr/simple-cache-implementation": "1.0"
    },
    "require": {
        "php": ">= 5.6",
        "psr/simple-cache": "^1"
    },
    "require-dev": {
        "codeception/codeception": "^2",
        "mindplay/simple-cache": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Kodus\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kodus\\Cache\\Test\\": "tests/",
            "Kodus\\Cache\\Test\\Integration\\": "tests/integration/"
        }
    }
}
